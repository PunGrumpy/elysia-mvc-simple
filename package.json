{
  "name": "elysia-mvc-simple",
  "version": "1.0.50",
  "module": "src/index.js",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "dev:dockerdbs": "docker compose -f docker/docker-compose.yml up -d",
    "prepare": "husky install",
    "lint": "lint-staged",
    "format": "prettier --write .",
    "db": "prisma migrate dev && prisma generate",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@elysiajs/swagger": "^0.7.4",
    "@prisma/client": "^5.5.2",
    "elysia": "latest"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "bun-types": "latest",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "prisma": "^5.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
